<resources>

  <!-- Base application theme. -->
  <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
    <!-- Customize your theme here. -->
    <item name="colorPrimary">@color/color_primary</item>
    <item name="colorPrimaryDark">@color/color_primary_dark</item>
    <item name="colorAccent">@color/color_accent</item>
  </style>

  <style name="FabButtonStyle">
    <item name="fab:fab_size">normal</item>
    <item name="fab:fab_colorNormal">@color/fab_button_normal</item>
    <item name="fab:fab_colorPressed">@color/fab_button_pressed</item>
    <item name="fab:fab_colorRipple">@color/fab_button_ripple</item>
  </style>

</resources>
